{
  "author": "William Orr <will@worrbase.com> (http://worrbase.com/)",
  "name": "imdb-api",
  "description": "Queries unofficial imdb APIs to get movie and television information from imdb",
  "version": "1.3.4",
  "main": "lib/imdb.js",
  "homepage": "https://github.com/worr/node-imdb-api",
  "repository": {
    "type": "git",
    "url": "https://github.com/worr/node-imdb-api"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc -m commonjs --outDir lib src/imdb.ts",
    "prepublish": "tsc -m commonjs --outDir lib src/imdb.ts",
    "lint": "tslint src/imdb.ts",
    "test": "nodeunit test",
    "clean": "rm -rf node_modules lib"
  },
  "devDependencies": {
    "nodeunit": "0.x.x",
    "nock": "8.x.x",
    "tslint": "^3.15.1",
    "typescript": "1.8.x"
  },
  "optionalDependencies": {},
  "engines": {
    "node": ">=4.0"
  },
  "keywords": [
    "imdb",
    "movies",
    "search",
    "tv",
    "television"
  ],
  "dependencies": {
    "es6-promise": "^3.2.1"
  }
}
